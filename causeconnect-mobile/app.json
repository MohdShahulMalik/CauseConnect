{
  "expo": {
    "name": "CauseConnect",
    "slug": "causeconnect-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icons/ios-light.png",
    "scheme": "causeconnect",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": {
        "light": "./assets/images/icons/ios-light.png",
        "dark": "./assets/images/icons/ios-dark.png",
        "tinted": "./assets/images/icons/ios-tinted.png"
      },
      "bundleIdentifier": "com.merzah.causeconnect"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.merzah.causeconnect"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-auth0",
        {
          "domain": "dev-zczlap588q6otywu.au.auth0.com",
          "customScheme": "causeconnect"
        }
      ],
      [
        "react-native-bootsplash",
        {
          "assetsDir": "./assets/bootsplash",
          "android": {
            "parentTheme": "EdgeToEdge",
            "darkContentBarsStyle": false
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fb5910e6-6c93-449c-ba03-d0663dff8c24"
      }
    }
  }
}
